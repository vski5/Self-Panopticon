{
  "name": "@maticnetwork/wallet-widget",
  "version": "1.3.12",
  "description": "Wallet widget is a light wallet which can be embedded in any sites for executing transaction on Polygon.",
  "main": "dist/npm.export.js",
  "types": "dist/ts/widget/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --hot",
    "dev:mainnet": "cross-env BUILD_ENV=mainnet npm run dev",
    "build": "npm run build:el:dev && npm run build:widget",
    "build:link": "npm run build && npm link",
    "build:prod": "npm run build:el:prod && npm run build:widget",
    "build:widget": "webpack --config widget/webpack/webpack.config.dev.js",
    "build:widget:dev": "cross-env NODE_ENV=development npm run build:widget",
    "build:el": "webpack --config webpack.config.js && node build_helper/create_manifest.js",
    "build:el:dev": "cross-env NODE_ENV=development npm run build:el",
    "build:el:prod": "cross-env NODE_ENV=production npm run build:el",
    "deploy": "node build_helper/delete_dist.js && npm run build:prod",
    "upload:assets": "node client_deploy.js",
    "analyze": "webpack --profile --json > stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maticnetwork/wallet-widget.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maticnetwork/wallet-widget/issues"
  },
  "homepage": "https://github.com/maticnetwork/wallet-widget#readme",
